<table class="table table-hover">
  <thead>
    <tr class="table-secondary">
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
      <th>ジャンル</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <% items.each do |item| %>
  <tbody>
    <tr>
      <th><%= item.id %></th>
      <th>
        <%= link_to admin_item_path(item.id) do %>
          <%= item.name %>
        <% end %>
      </th>
      <th><%= item.price_non_tax %></th>
      <th><%= item.genre.name %></th>
      <th><%= item.is_active %></th>
    </tr>
  </tbody>
  <% end %>
</table>